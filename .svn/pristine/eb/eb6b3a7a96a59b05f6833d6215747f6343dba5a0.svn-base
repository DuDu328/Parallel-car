.header img{
	float:left;
	width:16px;
	vertical-align: center;
}
.header{
	text-align:center;
	font-size:18px;
}

/*main*/
.main1{
	display: flex;
	flex-direction:row;
	justify-content: space-between;
}

/*left*/

.left{
	flex:2;
}
.list-brand{
	border:solid 1px #eaeaea;
	margin:0;
	padding:0;
}
.list-brand li{
	margin:0;
	padding:0;
	height:44px;
	line-height:44px;
	padding-left:8px;
	font-size:14px;
}
.list-brand li.active{
	background:#f4f4f4;
	border-left:2px solid #775687;
	padding-left:8px;
}
.list-brand li img{
	width: 26px;
	margin-right: 5px;
	vertical-align: middle;
}
.list-brand{
	position: fixed;
	width:28%;

}
/*right*/
.right{
	flex: 5;
	padding:0 0px 0 10px;
	
}
.list-type{
	/*display: flex;
	flex-direction:row;
	flex-wrap:wrap;
	justify-content: space-around;*/
	margin-top: 10px;
}
.list-type dl{
	text-align: center;
    width: 46%;
    margin-bottom: 10px;
    float: left;
    margin-right: 4%;
    font-size:14px;
	
}
.list-type dl dt{
	width: 100%;
    height: 86px;
    background: #f5f5f6;
    border-radius: 5px;
    margin-bottom:10px;
    overflow: hidden;
}
.list-type dl dt img{
	width:84%;
}